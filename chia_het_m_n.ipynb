    "M = int(input('Nhap M:'))\n",
    "N = int(input('Nhap N:'))\n",
    "\n",
    "tong = 0\n",
    "so = M\n",
    "while so <= N:\n",
    "    if so % 3 == 0 and so % 5 == 0:\n",
    "        tong = tong + so\n",
    "        pass \n",
    "    so = so + 1\n",
    "    pass\n",
    "\n",
    "print(tong)"
   ]
